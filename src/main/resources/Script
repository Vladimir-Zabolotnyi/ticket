CREATE TABLE journey
(
    journey_id  SERIAL NOT NULL PRIMARY KEY ,
    station_from VARCHAR(30)  NOT NULL,
    station_to VARCHAR(30) NOT NULL,
    departure TIMESTAMP NOT NULL,
    arrival TIMESTAMP NOT NULL
)

create or replace function find_all(p_db_name IN varchar(20)) returns refcursor as
$$
declare
    db_cursor refcursor;
begin
   open db_cursor for execute format('select * from %I', p_db_name) ;
   return  db_cursor;
end;
$$ language plpgsql;